{
  "name": "enjanga-core-setup",
  "version": "1.0.25",
  "description": "Unified dependency manager for all Enjanga projects. Bundles the full Carbon Design System with required peer dependencies and utilities.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "style": "src/styles/index.scss",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./next": {
      "import": "./dist/next.mjs",
      "require": "./dist/next.js",
      "types": "./dist/next.d.ts"
    },
    "./styles": {
      "sass": "./src/styles/index-no-fonts.scss",
      "default": "./src/styles/index-no-fonts.scss"
    },
    "./styles-with-fonts": {
      "sass": "./src/styles/index.scss",
      "default": "./src/styles/index.scss"
    },
    "./carbon-css": {
      "import": "./dist/styles-no-fonts.min.css",
      "require": "./dist/styles-no-fonts.min.css"
    },
    "./carbon-css-with-fonts": {
      "import": "./dist/styles.min.css",
      "require": "./dist/styles.min.css"
    },
    "./tsup.shared.js": {
      "import": "./tsup.shared.js",
      "require": "./tsup.shared.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/styles",
    "tsup.shared.js",
    "tsup.shared.d.ts",
    "dist/styles.min.css",
    "styles.scss"
  ],
  "scripts": {
    "build": "tsup && npm run build-css",
    "build-css": "npm run copy-carbon-css && npm run build-carbon-css-no-fonts",
    "copy-carbon-css": "cp node_modules/@carbon/styles/css/styles.min.css dist/styles.min.css",
    "build-carbon-css-no-fonts": "sass --load-path=node_modules src/styles/carbon-no-fonts.scss dist/styles-no-fonts.min.css --style=compressed --no-source-map",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run type-check",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf node_modules dist && yarn cache clean && yarn install",
    "clean:hard": "rm -rf node_modules dist yarn.lock && yarn cache clean && yarn install",
    "release": "yarn clean:hard && yarn build && npm version patch && npm publish"
  },
  "dependencies": {
    "@carbon/icons-react": "^11.67.0",
    "@carbon/pictograms-react": "^11.67.0",
    "@carbon/react": "^1.91.0",
    "@carbon/styles": "^1.90.0",
    "@contentful/rich-text-types": "^17.1.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "axios": "^1.11.0",
    "carbon-components": "^10.58.15",
    "clsx": "^2.1.1",
    "enjanga-components-library": "^1.0.12",
    "sass": "^1.94.2",
    "smoothscroll-polyfill": "^0.4.4",
    "wicg-inert": "^3.1.3"
  },
  "devDependencies": {
    "@types/carbon__pictograms-react": "^11.89.0",
    "@types/carbon__themes": "^0.0.3",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/smoothscroll-polyfill": "^0.3.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0",
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "keywords": [
    "carbon",
    "design-system",
    "react",
    "nextjs",
    "components",
    "ibm",
    "react-query",
    "axios"
  ],
  "author": "Enjanga <contact@enjanga.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericnjanga/enjanga-core-setup.git"
  },
  "bugs": {
    "url": "https://github.com/ericnjanga/enjanga-core-setup/issues"
  },
  "homepage": "https://github.com/ericnjanga/enjanga-core-setup#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ]
}
