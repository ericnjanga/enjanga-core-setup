{
  "name": "enjanga-core-setup",
  "version": "1.0.6",
  "description": "Complete Carbon Design System dependency bundle with all required peers and utilities",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "style": "src/styles/index.scss",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": {
      "sass": "./src/styles/index.scss",
      "default": "./src/styles/index.scss"
    }
  },
  "files": [
    "dist",
    "src/styles",
    "tsup.shared.js"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "dev": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run type-check",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@carbon/icons-react": "^11.67.0",
    "@carbon/pictograms-react": "^11.67.0",
    "@carbon/react": "^1.91.0",
    "@carbon/styles": "^1.90.0",
    "@contentful/rich-text-types": "^17.1.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "axios": "^1.11.0",
    "carbon-components": "^10.58.15",
    "clsx": "^2.1.1",
    "sass": "^1.89.2",
    "smoothscroll-polyfill": "^0.4.4",
    "wicg-inert": "^3.1.3"
  },
  "devDependencies": {
    "@types/carbon__pictograms-react": "^11.46.16",
    "@types/carbon__themes": "^0.0.3",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/smoothscroll-polyfill": "^0.3.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0",
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "keywords": [
    "carbon",
    "design-system",
    "react",
    "nextjs",
    "components",
    "ibm",
    "react-query",
    "axios"
  ],
  "author": "Enjanga <contact@enjanga.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericnjanga/enjanga-core-setup.git"
  },
  "bugs": {
    "url": "https://github.com/ericnjanga/enjanga-core-setup/issues"
  },
  "homepage": "https://github.com/ericnjanga/enjanga-core-setup#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ]
}
